{
    "name": "php-spl/skeleton",
    "type": "library",
    "description": "PHP default web project.",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/php-spl/framework"
        }
    ],
    "require": {
        "php-spl/framework": "dev-main"
    },
    "config": {
        "optimize-autoloader": false
    },
    "autoload": {
        "psr-4": {
            "Spl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    }
}
